<Window x:Class="AspNetAuthentication.WpfClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AspNetAuthentication.WpfClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="400">
    <StackPanel>
        <Button x:Name="ButtonSignIn" Content="Sign In" Click="ButtonSignIn_Click" />
        <Button x:Name="ButtonSignOut" Content="Sign Out" Click="ButtonSignOut_Click" Visibility="Collapsed" />
    </StackPanel>
</Window>
